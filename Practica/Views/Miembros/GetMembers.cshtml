@model IEnumerable<Practica.Models.Miembro>

    ViewBag.Title = "GetMembers";

  <div class="container">

        <h2 class="text-center">Listado de Miembros</h2>

        <div style="padding-bottom:2.5%; padding-top:1.5%;">
            @Html.DropDownList("Miembros", (IEnumerable<SelectListItem>)ViewBag.Members, new { @class = "form-control", @id = "members" })
        </div>

        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Miembro</th>
                    <th>Asignatura</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(modelitem => item.nombre_miembro)</td>
                        <td>@Html.DisplayFor(modelitem => item.Asignatura.nombre_asig)</td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
